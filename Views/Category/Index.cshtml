@model List<Category>
<div class="container">
    <div class="row pt-4 pb-3">
        <div class="col-6">
            <h3 class="text-primary">
                Category List
            </h3>
        </div>
        <div class="col-6 text-end">
            <h3 class="text-primary">
                <a asp-controller="Category" asp-action="Create" class="btn btn-primary"><i class="bi bi-plus"></i>Add New Category</a>
            </h3>
        </div> 
    </div>
<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Category Name</th>
            <th scope="col">Display Order</th>            
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model){
        <tr>
             <td>@obj.id</td>
            <td>@obj.name</td>
            <td>@obj.displayorder</td>            
            
        </tr>   
        }        
    </tbody>
</table>
</div>  